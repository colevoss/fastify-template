  "engines": {
    "node": ">=16.0.0"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@tsconfig/node16": "^1.0.2",
    "@types/config": "^0.0.41",
    "@types/jest": "^27.0.3",
    "@types/node": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "eslint": "^8.3.0",
    "husky": "^7.0.4",
    "jest": "^27.4.2",
    "lint-staged": "^12.1.2",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.2.3"
  },
  "lint-staged": {
    "*.{ts,js}": "eslint --cache --fix",
    "*.{ts,js,md}": "prettier --write"
